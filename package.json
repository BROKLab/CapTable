{
  "name": "@brok/captable-contracts",
  "version": "1.3.0",
  "description": "Contract artifacts for brok captables",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "run-s deploy:brokTest  && run-s deploy:brokStage && run-s build",
    "copy-dts": "copyfiles -u 1 \"src/typechain/**/*.d.ts\" ./lib",
    "build": "rm -rf lib && tsc -p tsconfig.release.json && run-s copy-dts",
    "deploy:brokTest": "npx hardhat deploy --network brokTest --export src/DeploymentsBrokTest.json",
    "deploy:brokStage": "npx hardhat deploy --network brokStage --export src/DeploymentsBrokStage.json",
    "create:brokTest:captable": "hh run scripts/createCapTable.ts --network brokTest",
    "create:brokStage:captable": "hh run scripts/createCapTable.ts --network brokStage"
  },
  "files": [
    "lib/"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.1.0",
    "@typechain/ethers-v5": "^7.0.0",
    "@typechain/hardhat": "^2.0.1",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "chai": "^4.3.4",
    "copyfiles": "^2.4.1",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.2.0",
    "hardhat": "^2.3.0",
    "hardhat-deploy": "^0.7.10",
    "hardhat-deploy-ethers": "^0.3.0-beta.7",
    "mocha": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "openzeppelin-solidity": "2.1.3",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.0.0",
    "typechain": "^5.0.0",
    "typescript": "^4.3.2"
  }
}